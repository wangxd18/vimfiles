priority -50

snippet if "if"
if (${1:/* condition */}) {
	${2:// code...}
}
endsnippet

snippet else "else"
else {
	${1:// code...}
}
endsnippet

snippet ife "if else"
if (${1:/* condition */}) {
	${2:// code...}
} else {
	${3:// code...}
	$0
}
endsnippet

snippet l "lambda" w
(${1:x}) ==> $1 ${VISUAL}$0
endsnippet

snippet al "async lambda" w
aysnc (${1:x}) ==> await $1 ${VISUAL}$0
endsnippet

